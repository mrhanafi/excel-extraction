import re
import io
import json
import common_logger
import column_helper
import odbc_driver_version
import urllib.parse
import pandas as pd
import numpy as np
import time
import osdu_mapping_helper
from datetime import datetime
from azure.storage.blob import BlobServiceClient, BlobClient, ContainerClient
from sqlalchemy import create_engine, Engine, types
from urllib.parse import quote_plus


# Convert column name to upper and replace whitespace with underscore
def standardize_column_format(df: pd.DataFrame,
                              custom_naming: dict[str,
                                                  str] = {}) -> pd.DataFrame:
    df.columns = [transform_column_name(x, custom_naming) for x in df.columns]
    return df


def transform_column_name(name: str,
                          custom_naming: dict[str, str] = {}) -> str:
    return (custom_naming[name] if name in custom_naming else "".join(
        map(str.capitalize, re.split(r"[\s_]", name))))


def place_column_next_to(df: pd.DataFrame, next_to: str, col_name: str):
    columns = df.columns.tolist()
    val = columns.pop(columns.index(col_name))
    order_index = columns.index(next_to) + 1
    columns.insert(order_index, val)
    return df[columns]


def place_columns_next_to(df: pd.DataFrame, columns_next_to: list[tuple[str,
                                                                        str]]):
    columns = df.columns.tolist()
    for next_to, col_name in columns_next_to:
        val = columns.pop(columns.index(col_name))
        order_index = columns.index(next_to) + 1
        columns.insert(order_index, val)
    return df[columns]


def translate_datatype(datatype: str):
    match datatype:
        case "NUMBER":
            return types.FLOAT
        case "STRING":
            return types.VARCHAR
        case "DATE":
            return types.DATE
        case _:
            raise ValueError(f"Unsupported data datatype: {datatype}")


def raw_extract_path(path_str: str) -> tuple[str, str, str]:
    return path_str.rsplit("/", 2)


def raw_try_get_unique_path(container_client: ContainerClient,
                            path_str: str,
                            max_retry=10) -> (BlobClient, str):
    retry = 0
    new_output_path = path_str
    while retry < max_retry:
        blob_client = container_client.get_blob_client(new_output_path)
        if blob_client.exists():
            retry += 1
            new_output_path = path_str.replace(".parquet", f"_{retry}.parquet")
            continue
        else:
            return blob_client, new_output_path


def insert_uom_columns(
        df: pd.DataFrame, uom_columns: list[tuple[str, str | None,
                                                  str]]) -> pd.DataFrame:
    for col, col_uom, unit in uom_columns:
        if not isinstance(col_uom, str):
            col_uom = col + "_UOM"
        df[col_uom] = unit
    df = place_columns_next_to(
        df,
        [(col, col_uom if isinstance(col_uom, str) else col + "_UOM")
         for col, col_uom, _ in uom_columns],
    )
    return df


def get_mssql_engine_from(json_str: str) -> Engine:
    json_data = json.loads(json_str)

    MSSQL_SERVER = json_data["Server"]
    MSSQL_DATABASE = json_data["Database"]
    MSSQL_USERNAME = json_data["Username"]
    MSSQL_PASSWORD = quote_plus(json_data["Password"])

    MSSQL_CONNECTION_STRING = (
        f"mssql+pyodbc://{MSSQL_USERNAME}:{MSSQL_PASSWORD}@{MSSQL_SERVER}/{MSSQL_DATABASE}"
        f"?driver=ODBC+Driver+{odbc_driver_version.odbc_driver_version()}+for+SQL+Server"
        "&TrustServerCertificate=yes")
    return create_engine(MSSQL_CONNECTION_STRING, isolation_level="AUTOCOMMIT")


def get_blob_service_container_client_from(json_str: str) -> ContainerClient:
    json_data = json.loads(json_str)
    account_name = json_data["AccountName"]
    sas_token = json_data["SasToken"]
    container_name = json_data["DefaultContainer"]

    blob_service_client = BlobServiceClient(
        account_url=f"https://{account_name}.blob.core.windows.net",
        credential=sas_token,
    )
    return blob_service_client.get_container_client(container_name)


# Blob management needs to duplicate file with new name, and then only delete the old one.
def rename_blob(new_blob_name: str, ori_blob_client: BlobClient,
                container_client: ContainerClient) -> None:
    new_blob_client = container_client.get_blob_client(new_blob_name)
    new_blob_client.start_copy_from_url(ori_blob_client.url)

    # Wait for the copy operation to complete
    props = new_blob_client.get_blob_properties()
    while props.copy.status == "pending":
        props = new_blob_client.get_blob_properties()

    # Delete the original blob
    ori_blob_client.delete_blob()


def ensure_datatype(
        df: pd.DataFrame,
        entity_datatype: dict[str, tuple[any, bool]]) -> pd.DataFrame:
    for col, (dtype, nullable) in entity_datatype.items():
        if not nullable and df[col].isnull().any():
            raise ValueError(f"Column {col} cannot be null")
        if dtype is not None:
            match dtype:
                case types.VARCHAR:
                    df[~df[col].isna()][col] = df[~df[col].isna()][col].astype(
                        str)
                case types.INT:
                    df[col] = df[col].astype(int)
                case types.FLOAT | types.DOUBLE:
                    df[col] = df[col].astype(float)
                case types.DATETIME | types.DATE:
                    df[col] = df[col].astype(str)
                    df[col] = pd.to_datetime(df[col], format="mixed")
                case _:
                    raise ValueError(f"Unsupported datatype: {dtype}")
    return df


def standardize_osdu_mapping(
        mapping_df: pd.DataFrame, df: pd.DataFrame,
        osdu_mapping_list: list[tuple[str, str, str, str]]) -> pd.DataFrame:
    if len(osdu_mapping_list) == 0:
        return df

    standardized_list = [(x if len(x) == 4 else
                          (x[0], x[1], x[2], None) if len(x) == 4 else
                          (x[0], x[1], None, None)) for x in osdu_mapping_list]
    for ref_table, sor_col, ref_col, der_col in standardized_list:
        print(f"Mapping for {sor_col} with {ref_col} from {ref_table}")
        start_time = time.time()

        table_query = f"Table == '{ref_table}'"
        if isinstance(ref_col, str):
            table_query += f" & ColumnName == '{ref_col}'"
        table_df = mapping_df.query(table_query)

        if not isinstance(der_col, str):
            der_col = f"DERIVED_{sor_col}"

        df = osdu_mapping_helper.derive_osdu_mapping_column(
            df, table_df, sor_col, der_col)

        df = place_column_next_to(df, sor_col, der_col)
        print(f"Mapping {sor_col} took {time.time() - start_time:.3f}s")

    return df


