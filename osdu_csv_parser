#below is the code to include required libraries, please check and add libraries at Administration > Settings > Python > Python Modules
import json
import base64
import requests
import os
from datetime import datetime, timedelta, date
import common_logger
from io import BytesIO

def get_target_date():
    # Get the first day of current month
    first_day_current = date.today().replace(day=1)

    # Subtract one day to get last day of previous month
    last_day_previous = first_day_current - timedelta(days=1)

    # Get first day of previous month
    first_day_previous = last_day_previous.replace(day=1)
    
    #first_day_previous = date(2024,11,1)
    
    return first_day_previous

def print_info(log_output, file_id, correlation_id, run_id, status_response):
    json_to_print = {
        "file_id": file_id,
        "correlation_id": correlation_id,
        "run_id": run_id,
        "status_response": status_response
    }
    
    log_output = common_logger.log(log_output, f"Logging required informations:\n{json.dumps(json_to_print, indent=4)}")
    return log_output

def extract_country(file_path):
    components = file_path.split('/')
    extracted_country = ""
    
    if len(components) >= 3:
        extracted_country = components[2].upper()
        
    return extracted_country

def convert_credential_b64(client_id, client_secret):
    client_id_n_secret = f'{client_id}:{client_secret}'
    raw_bytes = client_id_n_secret.encode()
    b64_bytes = base64.urlsafe_b64encode(raw_bytes)
    b64_string = b64_bytes.decode()
    
    return b64_string

def get_delfi_token(url, scope, auth_b64_string):
    payload=f'grant_type=client_credentials&scope={scope}'
    
    headers = {
        'Content-Type': 'application/x-www-form-urlencoded',
        'Authorization': f'Basic {auth_b64_string}'
    }

    response = requests.post(url, headers=headers, data=payload)
    json_response = json.loads(response.text)
    access_token = json_response["access_token"]
    return access_token

def validate_delfi_token(url, token, scope):
    payload = {
        "token": token,
        "audience": scope
    }

    headers = {
        'Content-Type': 'application/json'
    }

    response = requests.post(url, headers=headers, data=json.dumps(payload))
   
    json_response = json.loads(response.text)
    pretty_response = json.dumps(response.json(), indent=4, sort_keys=True)
    token_expiry_epoch = json_response["exp"]

    return token_expiry_epoch

def get_signed_landing_url(url, token, data_partition_id='petronas-osdu'):
    # Define API details
    headers = {'accept': 'application/json',  
               'data-partition-id': data_partition_id,  
               'Content-Type': 'application/json',  
               'Authorization': 'Bearer '+ token}

    # Make the GET request
    response = requests.get(url, headers=headers)
    
    result = {}

    # Check the response
    if response.status_code == 200:
        # Parse the JSON response
        response_data = response.json()
        result['signed_url'] = response_data["Location"]["SignedURL"]
        result['file_source'] = response_data["Location"]["FileSource"]
    else:
        raise Exception(f"Failed to get signed url, status code: {response.status_code}, response: {response.text}")
        
    return result

def upload_csv(signed_url, file_data):
    # Prepare headers for the PUT request
    headers = {
        'x-ms-blob-type': 'BlockBlob',  # Required for Azure Blob Storage
        'Content-Type': 'text/csv'     # Ensure the content type matches your file type
    }
    
    if isinstance(file_data, BytesIO):
        data = file_data.getvalue()
    else:
        data = file_data

    response = requests.put(signed_url, headers=headers, data=data)

    # Check the response
    if response.status_code == 201:
        print("File uploaded successfully!")
    else:
        raise Exception(f"Failed to upload csv to signed url, status code: {response.status_code}, response: {response.text}")
        
    return response

def get_file_metadata(filename, file_data):
    file_metadata = {}
    
    # Get current timestamp
    current_time = datetime.now().strftime('%Y-%m-%dT%H:%M:%S.%f')[:-3] + "Z"
    
    # If file_data is BytesIO or bytes, get its size
    if isinstance(file_data, BytesIO):
        file_size = len(file_data.getvalue())
    elif isinstance(file_data, bytes):
        file_size = len(file_data)
    else:
        file_size = len(file_data)
    
    file_metadata = {
        'file_name': filename,
        'file_size': file_size,
        'time_created': current_time,
        'time_updated': current_time
    }
    
    return file_metadata

def create_file_metadata(api_url, token, payload, correlation_id=None, data_partition_id='petronas-osdu'):
    headers = {
        'accept': 'application/json',  
        'data-partition-id': data_partition_id,  
        'Content-Type': 'application/json',  
        'Authorization': 'Bearer '+ token
    }
    
    # Only add correlation-id to headers if it's provided
    if correlation_id:
        headers['correlation-id'] = correlation_id
    
    # Make the POST request to create metadata
    response = requests.post(api_url, headers=headers, json=payload)
    # Check the response
    if response.status_code == 201:
        file_id = response.json().get("id")
    else:
        raise Exception(f"Failed to create metadata. status code: {response.status_code}, response: {response.text}")
    
    return file_id

def initiate_csv_parser(csv_parser_url, token, file_id, data_partition_id='petronas-osdu'):
    output = {}
    
    # Headers for the API request
    headers = {
        'accept': 'application/json',  
        'data-partition-id': data_partition_id,  
        'Content-Type': 'application/json',  
        'Authorization': 'Bearer '+ token
    }

    payload = {
        "executionContext": {
            "dataPartitionId": data_partition_id, 
            "id": file_id
        }
    }

    response = requests.post(csv_parser_url, headers=headers, json=payload)

    # Check the response
    if response.status_code == 200:
        output['run_id'] = response.json().get("runId")
        output['correlation_id'] = response.headers.get("correlation-id")
    else:
        raise Exception(f"Failed to initiate CSV Parser. status code: {response.status_code}, response: {response.text}")
    
    return output

def check_run_status(base_url, run_id, token, data_partition_id='petronas-osdu'):
    combined_url = f"{base_url}/{run_id}"
    
    header = {
        "Authorization": f"Bearer {token}",
        "data-partition-id": data_partition_id
    }
    
    # Send the GET request
    response = requests.get(combined_url, headers=header)

    # Check if the response is successful
    response.raise_for_status()
    return response.json()

